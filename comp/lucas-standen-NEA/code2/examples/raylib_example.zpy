(defun main int)
	(InitWindow 800 800 "test")
	(SetTargetFPS 40)
	(let x:int 0)
	(let y:int 0)
	(for i:int 0 (= (WindowShouldClose) 0) 0)
		(BeginDrawing)
		(ClearBackground RAYWHITE)
		(DrawText "test" x y 40 RED)
		(EndDrawing)

		(set x (+ x 5))
		(set y (+ y 5))
	(endfor)
	(CloseWindow)
(endfun)
